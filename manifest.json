{
  "name": "Upworthless",
  "version": "1.0",
  "manifest_version": 2,
  "background": "background.html",
  "permissions": [
    "tabs", "http://*.upworthy.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.upworthy.com/*"],
      "js": ["upworthless.js"],
      "run_at": "document_end"
    }
  ]
}